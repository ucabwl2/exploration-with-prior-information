data_set=oru-field
roslaunch graph_map visualize_graph_fuser.launch mapping:=true oru-field:=true "$data_set":=true &
rosrun graph_map  graph_mapping_offline --no-odometry --disable-submaps --itrs 50  --resolution 0.5  --registration3d  --resolution-local-factor 1.0  --min-range 1.3 --max-range 130 --base-name off --dir-name /mnt/storage/rosbag_storage/oru_bags/oru_field --lidar-topic /velodyne_packets --velodyne-frame-id /velodyne --map-type-name ndt_map  --velodyne-config-file "$(rospack find graph_map)/config/velo32.yaml" --output-dir-name /home/$USER/.ros/maps/  --interchange-radius 4 --compound-radius 0 --map-size-xy 450 --map-size-z 13.0 --skip-frame 1  --data-set $data_set  --visualize --disable-keyframe-update --keyframe-min-distance 0 --keyframe-min-rot-deg 0
